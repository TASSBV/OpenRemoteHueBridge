<!--
 |  Universal Devices ISY-99 gateway configuration for OpenRemote Designer.
 |
 |  Author: Andrew Ball
 +-->
<openremote xmlns="http://www.openremote.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openremote.org protocol.xsd">
	
  <protocol displayName="ISY-99" tagName="isy99">
    <attr name="address" label="Address">
      <validations>
        <allowBlank>false</allowBlank>
        <regex message="Address is required">.*</regex>
      </validations>
    </attr>

    <attr name="command" label="Command">
      <validations>
        <allowBlank>false</allowBlank>
        <regex message="Command is required">.*</regex>
      </validations>
    </attr>
  </protocol>

</openremote>
